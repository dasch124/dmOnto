                <item>Network drive</item>
                <item>local HDD</item>
                <item>local SSD</item>
                <item>external HDD</item>
                <item>external SSD</item>
                <item>CD-ROM</item>
                <item>DVD-ROM</item>
                <item>Tape</item>
            </list>
        </values>
    </vocab>
</vocabularies>
